<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- standard, dark -->
        <theme>dark</theme>
    
        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>
        
        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <protocol>https</protocol>
        
        <!-- title of the document -->
        <documentTitle>TestAutomation</documentTitle>
        
        <!-- report name - displayed at top-nav -->
        <reportName>Automation Test Report </reportName>
        
        <!-- report headline - displayed at top-nav, after reportHeadline -->
        <reportHeadline>---- NGS</reportHeadline>
        
        <!-- global date format override -->
        <!-- defaults to yyyy-MM-dd -->
        <dateFormat>yyyy-MM-dd</dateFormat>
        
        <!-- global time format override -->
        <!-- defaults to HH:mm:ss -->
        <timeFormat>HH:mm:ss</timeFormat>
        
		<enableCatagoryView>true</enableCatagoryView>
		<enableAuthoreView>false</enableAuthoreView>
		<enableExceptionView>true</enableExceptionView>
		<enableTestRunnerLogsview>true</enableTestRunnerLogsview>
		
		  <!-- custom javascript -->
		<scripts>
			<![CDATA[
				$(document).ready(function() {
				$(".logo-content").hide();
				$(".logo-container").html("HCA");
				$(".collapsible").parent().attr('colspan',2);
				});
			]]>
		</scripts>
		
		
	</configuration>
</extentreports>